{
    "system": {
        "shutdown": {
            "patterns": [
                "shutdown pc",
                "turn off computer",
                "power off system",
                "shutdown"
            ],
            "cmd": {
                "windows": "shutdown /s /t 0",
                "linux": "shutdown -h now",
                "macos": "sudo shutdown -h now"
            },
            "confirm": true
        },
        "restart": {
            "patterns": [
                "restart pc",
                "reboot computer",
                "restart system"
            ],
            "cmd": {
                "windows": "shutdown /r /t 0",
                "linux": "reboot",
                "macos": "sudo shutdown -r now"
            },
            "confirm": true
        },
        "lock_screen": {
            "patterns": [
                "lock screen",
                "lock workstation"
            ],
            "cmd": {
                "windows": "rundll32.exe user32.dll,LockWorkStation",
                "linux": "gnome-screensaver-command -l",
                "macos": "pmset displaysleepnow"
            }
        },
        "log_off": {
            "patterns": [
                "log off",
                "sign out"
            ],
            "cmd": {
                "windows": "shutdown /l",
                "linux": "gnome-session-quit --no-prompt",
                "macos": "osascript -e 'tell application \"System Events\" to log out'"
            },
            "confirm": true
        },
        "hibernate": {
            "patterns": [
                "hibernate",
                "deep sleep",
                "go to sleep",
                "sleep mode"
            ],
            "cmd": {
                "windows": "shutdown /h",
                "linux": "systemctl hibernate",
                "macos": "pmset sleepnow"
            },
            "confirm": true
        }
    },
    "network_advanced": {
        "ping": {
            "patterns": [
                "ping google",
                "check connection to google",
                "ping test"
            ],
            "cmd": {
                "windows": "ping google.com",
                "linux": "ping -c 4 google.com"
            }
        },
        "tracert": {
            "patterns": [
                "trace route",
                "tracert google",
                "trace path to google"
            ],
            "cmd": {
                "windows": "tracert google.com",
                "linux": "traceroute google.com"
            }
        },
        "nslookup": {
            "patterns": [
                "check dns",
                "nslookup google",
                "lookup domain"
            ],
            "cmd": {
                "windows": "nslookup google.com",
                "linux": "nslookup google.com"
            }
        },
        "flush_dns": {
            "patterns": [
                "flush dns",
                "clear dns cache"
            ],
            "cmd": {
                "windows": "ipconfig /flushdns",
                "linux": "sudo systemd-resolve --flush-caches",
                "macos": "sudo killall -HUP mDNSResponder"
            }
        },
        "renew_ip": {
            "patterns": [
                "renew ip",
                "refresh ip address"
            ],
            "cmd": {
                "windows": "ipconfig /renew",
                "linux": "sudo dhclient -r && sudo dhclient",
                "macos": "sudo ipconfig set en0 DHCP"
            }
        },
        "show_mac": {
            "patterns": [
                "show mac address",
                "get physical address"
            ],
            "cmd": {
                "windows": "getmac",
                "linux": "ip link show",
                "macos": "ifconfig | grep ether"
            }
        },
        "netstat_listen": {
            "patterns": [
                "show listening ports",
                "check open ports",
                "list listeners"
            ],
            "cmd": {
                "windows": "netstat -an | findstr LISTENING",
                "linux": "netstat -tuln",
                "macos": "lsof -i -P | grep LISTEN"
            }
        },
        "show_route": {
            "patterns": [
                "show routing table",
                "print routes",
                "route print"
            ],
            "cmd": {
                "windows": "route print",
                "linux": "ip route show",
                "macos": "netstat -rn"
            }
        }
    },
